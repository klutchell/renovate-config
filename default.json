{
  "extends": ["config:base"],
  "branchPrefix": "gh-renovate/",
  "allowPostUpgradeCommandTemplating": true,
  "allowedPostUpgradeCommands": ["^sed"],
  "platform": "github",
  "onboarding": false,
  "requireConfig": false,
  "repositories": [
    "klutchell/renovate-config",
    "klutchell/balena-syncthing",
    "klutchell/balena-nextcloud",
    "klutchell/balena-homeassistant",
    "klutchell/balena-pihole",
    "klutchell/balena-wireguard",
    "klutchell/balena-frigate",
    "klutchell/balena-adguard",
    "klutchell/mediaserver"
  ],
  "rebaseWhen": "auto",
  "automerge": false,
  "labels": ["renovate"],
  "printConfig": true
}

{
  "extends": [
    "config:base",
    ":semanticCommitsDisabled"
  ],
  "branchPrefix": "gh-renovate/",
  "allowPostUpgradeCommandTemplating": true,
  "allowedPostUpgradeCommands": [
    "^sed"
  ],
  "platform": "github",
  "prHourlyLimit": 0,
  "onboarding": false,
  "requireConfig": false,
  "autodiscover": true,
  "rebaseWhen": "behind-base-branch",
  "automerge": false,
  "labels": [
    "renovate"
  ],
  "printConfig": true,
  "regexManagers": [
    {
      "fileMatch": [
        "(^|\\/)(?:docker-)?compose[^/]*\\.ya?ml$",
        "(^|\\/)Dockerfile(\\.[[:alnum:]]+)?$"
      ],
      "matchStrings": [
        "bh\\.cr/gh_klutchell/tailscale-%%BALENA_ARCH%%/(?<currentValue>.*?)\\n"
      ],
      "depNameTemplate": "klutchell/balena-tailscale",
      "datasourceTemplate": "github-tags",
      "extractVersionTemplate": "^v?(?<version>.*)$"
    },
    {
      "fileMatch": [
        "(^|\\/)(?:docker-)?compose[^/]*\\.ya?ml$",
        "(^|\\/)Dockerfile(\\.[[:alnum:]]+)?$"
      ],
      "matchStrings": [
        "bh\\.cr/gh_klutchell/balena-restic-%%BALENA_ARCH%%/(?<currentValue>.*?)\\n"
      ],
      "depNameTemplate": "klutchell/balena-restic",
      "datasourceTemplate": "github-tags",
      "extractVersionTemplate": "^v?(?<version>.*)$"
    }
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "renovatebot/github-action"
      ],
      "matchBaseBranches": [
        "main"
      ],
      "automerge": true
    }
  ]
}
